@namespace Skclusive.Material.Form
@inherits FormControlLabelComponent

<Component
    RootRef="@RootRef"
    Tag="@Component"
    Class="@_Class"
    Style="@_Style"
    @attributes="Attributes">
    @ChildContent?.Invoke(ControlContext)
    <Typography
        Component="span"
        Class="@_LabelClass">
        @Label
    </Typography>
</Component>
