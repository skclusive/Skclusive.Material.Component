@namespace Skclusive.Material.Table
@inherits StyleComponentBase

<Styled
    Name="TableRow">

    .TableRow-Root {
        color: inherit;
        display: table-row;
        outline: 0;
        vertical-align: middle;
    }

    .TableRow-Root.TableRow-Selected {
        background-color: var(--theme-palette-table-row-background-selected, rgba(0, 0, 0, 0.04));
    }

    .TableRow-Root.TableRow-Hover:hover {
        background-color: var(--theme-palette-table-row-background-hover, rgba(0, 0, 0, 0.07));
    }

    .TableRow-Head {
    }

    .TableRow-Foot {
    }

    .TableRow-Body {
    }

</Styled>