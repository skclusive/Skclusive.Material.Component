@namespace Skclusive.Material.Paper
@inherits StyleComponentBase

<Styled
    Name="Paper">

    .Paper-Root {
        color: var(--theme-palette-text-primary, rgba(0, 0, 0, 0.87));
        transition: var(--theme-transition-box-shadow, box-shadow 300ms cubic-bezier(0.4, 0, 0.2, 1) 0ms);
        background-color: var(--theme-palette-background-paper, #fff);
    }

    .Paper-Rounded {
        border-radius: calc(var(--theme-shape-border-radius, 4) * 1px);
    }

    .Paper-Elevation0 {
        box-shadow: var(--theme-shadow0, none);
    }

    .Paper-Elevation1 {
        box-shadow: var(--theme-shadow1, 0px 2px 1px -1px rgba(0,0,0,0.2),0px 1px 1px 0px rgba(0,0,0,0.14),0px 1px 3px 0px rgba(0,0,0,0.12));
    }

    .Paper-Elevation2 {
        box-shadow: var(--theme-shadow2, 0px 3px 1px -2px rgba(0,0,0,0.2),0px 2px 2px 0px rgba(0,0,0,0.14),0px 1px 5px 0px rgba(0,0,0,0.12));
    }

    .Paper-Elevation3 {
        box-shadow: var(--theme-shadow3, 0px 3px 3px -2px rgba(0,0,0,0.2),0px 3px 4px 0px rgba(0,0,0,0.14),0px 1px 8px 0px rgba(0,0,0,0.12));
    }

    .Paper-Elevation4 {
        box-shadow: var(--theme-shadow4, 0px 2px 4px -1px rgba(0,0,0,0.2),0px 4px 5px 0px rgba(0,0,0,0.14),0px 1px 10px 0px rgba(0,0,0,0.12));
    }

    .Paper-Elevation5 {
        box-shadow: var(--theme-shadow5, 0px 3px 5px -1px rgba(0,0,0,0.2),0px 5px 8px 0px rgba(0,0,0,0.14),0px 1px 14px 0px rgba(0,0,0,0.12));
    }

    .Paper-Elevation6 {
        box-shadow: var(--theme-shadow6, 0px 3px 5px -1px rgba(0,0,0,0.2),0px 6px 10px 0px rgba(0,0,0,0.14),0px 1px 18px 0px rgba(0,0,0,0.12));
    }

    .Paper-Elevation7 {
        box-shadow: var(--theme-shadow7, 0px 4px 5px -2px rgba(0,0,0,0.2),0px 7px 10px 1px rgba(0,0,0,0.14),0px 2px 16px 1px rgba(0,0,0,0.12));
    }

    .Paper-Elevation8 {
        box-shadow: var(--theme-shadow8, 0px 5px 5px -3px rgba(0,0,0,0.2),0px 8px 10px 1px rgba(0,0,0,0.14),0px 3px 14px 2px rgba(0,0,0,0.12));
