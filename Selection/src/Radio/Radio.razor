@namespace Skclusive.Material.Selection
@inherits RadioComponent

<SwitchBase
    RootRef="@RootRef"
    ChildRef="@ChildRef"
    Id="@Id"
    Type="radio"
    Name="@_Name"
    Class="@_Class"
    Style="@_Style"
    Value="@Value"
    Color="@_Color"
    DefaultChecked="@DefaultChecked"
    Checked="@_Checked"
    Disabled="@Disabled"
    DisableRipple="@DisableRipple"
    TabIndex="@TabIndex"
    OnChange="@HandleChangeAsync"
    CheckedClass="@_CheckedClass"
    DisabledClass="@_DisabledClass"
    InputClass="@InputClass"
    InputStyle="@InputStyle"
    Icon="@_Icon"
    CheckedIcon="@_CheckedIcon"
    InputProps="@InputProps"
    @attributes="Attributes" />
